datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  id        Int      @id @default(autoincrement())
  nombre    String
  email     String   @unique
  pdfSent   Boolean  @default(false)
  createdAt DateTime @default(now())
}
